@font-face {
  font-family: 'Open Sans';
  src: url('<%= font_path 'open_sans/OpenSans-Regular.eot' %>');
  src: url('<%= font_path 'open_sans/OpenSans-Regular.eot' %>#iefix') format('embedded-opentype'),
       url('<%= font_path 'open_sans/OpenSans-Regular.woff2' %>') format('woff2'),
       url('<%= font_path 'open_sans/OpenSans-Regular.woff' %>') format('woff'),
       url('<%= font_path 'open_sans/OpenSans-Regular.ttf' %>')  format('truetype'),
       url('<%= font_path 'open_sans/OpenSans-Regular.svg' %>#OpenSans') format('svg');
  font-weight: normal;
  font-style: normal;
}

