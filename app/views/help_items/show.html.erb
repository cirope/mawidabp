<div id="showed_data">
<p>
  <b><%= HelpItem.human_attribute_name 'order_number' %>:</b>
  <%=h @help_item.order_number %>
</p>

<p>
  <b><%= HelpItem.human_attribute_name 'name' %>:</b>
  <%=h @help_item.name %>
</p>

<p>
  <b><%= HelpItem.human_attribute_name 'description' %>:</b>
  <%=h @help_item.description %>
</p>
</div>

<div id="links">
