<div id="control_objective_filter" <%== 'style="display: none;"' unless display %>>
  <div class="row">
    <div class="col-md-6">
      <div class="form-group row">
        <% control_objective_filter = params[action] ? params[action][:control_objective] : nil %>
        <%= f.label :control_objective, ControlObjective.model_name.human, class: 'col-md-4 col-form-label' %>
        <div class="col-md-8">
          <%= f.text_field :control_objective, value: control_objective_filter, maxlength: 3072, class: 'form-control' %>
        </div>
      </div>
    </div>
  </div>
</div>
