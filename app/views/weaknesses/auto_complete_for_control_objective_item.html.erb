<% unless @control_objective_items.empty? %>
  <ul>
    <% @control_objective_items.each do |coi| %>
      <li id="dynamic_result_<%= "#{rand(100000)}_id_#{coi.id}" %>">
        <%= highlight(h(coi.control_objective_text), @tokens) %>
        <span class="informal aditional_info">
          <%= highlight(h(coi.review.to_s), @tokens) %>
        </span>
      </li>
    <% end %>
  </ul>
<% else %>
  <ul style="display: none;"><li></li></ul>
  <p class="empty_result"><%= t(:'message.empty_result') %></p>
<% end %>