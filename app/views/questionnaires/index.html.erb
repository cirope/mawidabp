<table class="table table-condensed table-striped">
  <thead>
    <tr>
      <th><%= Questionnaire.human_attribute_name :name %></th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @questionnaires.each do |questionnaire| %>
      <tr>
        <td><%= questionnaire.name %></td>
        <td><%= link_to_show questionnaire %></td>
        <td><%= link_to_edit edit_questionnaire_path(questionnaire) %></td>
        <td><%= link_to_destroy questionnaire %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= pagination_links @questionnaires %>

<%= link_to t('label.new'), new_questionnaire_path, class: 'btn btn-primary' %>
