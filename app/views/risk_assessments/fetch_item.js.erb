+function () {
  var $container = $('[data-container-id="<%= params[:container] %>"]')

  <%= simple_fields_for @risk_assessment do |f| %>
    <%= f.simple_fields_for :risk_assessment_items, @risk_assessment_item, include_id: false, child_index: params[:index] do |rai_f| %>
      $container.html('<%= j render('risk_weights', f: rai_f) %>')
    <% end %>
  <% end %>
}()
