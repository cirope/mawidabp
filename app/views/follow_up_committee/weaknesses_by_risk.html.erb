<%= render :partial => 'shared/filter_form',
  :locals => {:action => :weaknesses_by_risk, :extra_params => {},
  :forms => {:date_filter => true}} %>

<div style="margin: 1em;">
  <% @audit_types.each do |type| -%>
    <h1 class="center_title">
      <%= t("conclusion_committee_report.weaknesses_type_#{type}") %>
    </h1>
    <%= render :partial => 'weaknesses_synthesis_table', :locals => {
      :table_data => @tables_data[type],
      :being_implemented_resume => @being_implemented_resumes[type]} %>
  <% end -%>
</div>

<div id="links">
  <%= link_to t(:'label.back'), :action => :index %> |
  <%= link_to t(:'label.download'), :action => :weaknesses_by_risk,
    :weaknesses_by_risk => {:from_date => @from_date, :to_date => @to_date},
    :download => 1 %>
</div>